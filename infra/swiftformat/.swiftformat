--swiftversion 5.6

# rules
--disable blankLinesAtEndOfScope
--disable blankLinesAtStartOfScope
--disable consecutiveBlankLines
--disable preferKeyPath
--disable redundantReturn
--disable redundantSelf
--disable redundanttype
--disable sortedImports

--enable redundantGet
--enable redundantInit
--enable linebreakAtEndOfFile

--allman false
--assetliterals visual-width
--beforemarks
--binarygrouping none
--closingparen balanced
--closurevoid preserve
--commas inline
--conflictmarkers reject
--decimalgrouping 3,6
--elseposition same-line
--exponentcase lowercase
--exponentgrouping disabled
--extensionacl on-declarations
--extensionmark "MARK: - %t + %c"
--fractiongrouping disabled
--fragment false
--funcattributes preserve
--guardelse auto
--header ignore
--hexgrouping none
--hexliteralcase uppercase
--patternlet inline
--ifdef no-indent
--indent 4
--indentcase false
--linebreaks lf
--markextensions always
--marktypes always
--maxwidth 140
--modifierorder override, acl, setterACL, dynamic, mutators, lazy, final, required, convenience, typeMethods, owned
--nevertrailing
--nospaceoperators
--nowrapoperators
--octalgrouping none
--operatorfunc spaced
--ranges no-space
--self init-only
--selfrequired min
--semicolons never
--shortoptionals always
--smarttabs enabled
--stripunusedargs closure-only
--tabwidth unspecified
--trailingclosures
--trimwhitespace nonblank-lines
--typeattributes preserve
--typemark "MARK: - %t"
--varattributes preserve
--voidtype void
--wraparguments before-first
--wrapcollections before-first
--wrapconditions preserve
--wrapparameters before-first
--wrapreturntype preserve
--xcodeindentation disabled
--yodaswap always